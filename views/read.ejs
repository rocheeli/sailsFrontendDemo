<%- partial ('partials/statusmessage.ejs') %>

<h2>Users</h2>
<table class="table  table-bordered">
  <tr class="success">
    <th>ID</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Password</th>
    <th>Email</th>
    <th>Home Number</th>
    <th>Cell Number</th>
    <th>Division</th>
    <th>Year of Service</th>
  </tr>
  <% if(typeof users != "undefined" && users.length > 0){ %>
    <% users.forEach(function(user){ %>
      <tr class="success">
        <td><%= user.id %></td>
        <td><%= user.firstName %></td>
        <td><%= user.lastName %></td>
        <td><%= user.password %></td>
        <td><%= user.email %></td>
        <td><%= user.homeNumber %></td>
        <td><%= user.cellNumber %></td>
        <td><%= user.division %></td>
        <td><%= user.yearsOfService %></td>
      </tr>
    <% }) %>
  <% }else{ %>
      <tr>
        <td colspan="9">0 records found</td>
      </tr>
  <% } %>
  
</table>


